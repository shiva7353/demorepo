<core:View xmlns:commons="sap.ui.commons" xmlns:core="sap.ui.core" xmlns:form="sap.ui.layout.form" xmlns:l="sap.ui.layout"
	xmlns:sfi="sap.ui.comp.smartfield" xmlns:sfo="sap.ui.comp.smartform" xmlns:table="sap.ui.table"
	xmlns:template="http://schemas.sap.com/sapui5/extension/sap.ui.core.template/1" xmlns:uxap="sap.uxap" 	xmlns="sap.suite.ui.microchart"
	>
	<ColumnMicroChart size="M" press="press" class="sapUiSmallMargin">
		<leftTopLabel>
			<ColumnMicroChartLabel label="0M" color="Good"/>
		</leftTopLabel>
		<rightTopLabel>
			<ColumnMicroChartLabel label="80M" color="Critical"/>
		</rightTopLabel>
		<leftBottomLabel>
			<ColumnMicroChartLabel label="June 1"/>
		</leftBottomLabel>
		<rightBottomLabel>
			<ColumnMicroChartLabel label="June 30"/>
		</rightBottomLabel>
		<columns>
			<ColumnMicroChartData value="60" color="Error"/>
			<ColumnMicroChartData value="80" color="Error"/>
			<ColumnMicroChartData value="100" color="Error"/>
			<ColumnMicroChartData value="60" color="Neutral"/>
			<ColumnMicroChartData value="55" color="Neutral"/>
			<ColumnMicroChartData value="15" color="Neutral"/>
		</columns>
	</ColumnMicroChart>	
</core:View>